{
    "name": "inanepain/cli",
    "description": "Utilities to simplify working with the console.",
    "type": "library",
    "license": "Unlicense",
    "homepage": "https://github.com/inanepain/cli",
    "readme": "README.md",
    "authors": [
        {
            "name": "Philip Michael Raab",
            "email": "philip@cathedral.co.za",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "philip@cathedral.co.za",
        "issues": "https://github.com/inanepain"
    },
    "keywords": [
        "inane",
        "cli",
        "console",
        "utility"
    ],
    "autoload": {
        "psr-4": {
            "Inane\\Cli\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "inanepain/stdlib": ">=0.4.4 || dev-master || dev-develop"
    },
    "require-dev": {}
}
